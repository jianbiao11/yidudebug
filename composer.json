{
    "name": "yidu/yii2-yidulog",
    "description": "The yidu extension for Yii framework debugger",
    "keywords": ["yii2", "yidu_debug", "debugger"],
    "type": "yii2-extension",
    "license": "BSD-2-Clause",
    "support": {
        "issues": "https://github.com/jianbiao11/yidudebug/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/jianbiao11/yidudebug"
    },
    "authors": [
        {
            "name": "jianbiao11<baseon Qiang Xue>",
            "email": "154530905@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.11",
        "yiisoft/yii2-bootstrap": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}
